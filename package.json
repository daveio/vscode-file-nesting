{
  "type": "module",
  "private": true,
  "packageManager": "bun@1.3.7",
  "author": "Dave Williams <dave@dave.io>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/daveio",
  "engines": {
    "bun": "^1.3.7"
  },
  "scripts": {
    "ext:build": "cd extension && tsdown src/index.ts --external vscode",
    "ext:dev": "nr ext:build --watch src",
    "ext:pack": "nr ext:build && cd extension && vsce package --no-dependencies",
    "ext:publish": "nr ext:build && cd extension && vsxpub --no-dependencies",
    "lint": "eslint .",
    "release": "cd extension && bumpp",
    "update": "node update.mjs",
    "vscode:prepublish": "nr ext:build"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@antfu/ni": "^28.2.0",
    "@types/vscode": "^1.108.1",
    "@vscode/vsce": "^3.7.1",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "eslint-factory": "^0.1.2",
    "ofetch": "^1.5.1",
    "ovsx": "^0.10.8",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "vsxpub": "^0.1.3"
  },
  "trustedDependencies": [
    "@vscode/vsce-sign"
  ]
}
